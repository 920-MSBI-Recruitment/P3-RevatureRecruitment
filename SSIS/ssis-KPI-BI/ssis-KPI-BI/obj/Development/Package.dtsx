    private int CurrentQlCount = 0;
    private DateTime BatchStartDate = DateTime.Today.AddDays(365);
    private static int WeeksWaited = 2;
    private int DaysWaited = 7 * WeeksWaited;
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        if (CurrentQlCount < 20 && Row.offerActDate.CompareTo(BatchStartDate) < 0)
        {
            if (Row.offerAction == "Accept")
            {
                CurrentQlCount++;
                if (CurrentQlCount == 12)
                {
                    switch (Row.offerActDate.DayOfWeek)
                    {
                        case DayOfWeek.Monday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited);
                            break;
                        case DayOfWeek.Tuesday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 6);
                            break;
                        case DayOfWeek.Wednesday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 5);
                            break;
                        case DayOfWeek.Thursday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 4);
                            break;
                        case DayOfWeek.Friday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 3);
                            break;
                        case DayOfWeek.Saturday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 2);
                            break;
                        case DayOfWeek.Sunday:
                            BatchStartDate = Row.offerActDate.AddDays(DaysWaited + 1);
                            break;
                    }
                }
            }
        }
        else
        {
            SqlConnection revDWConnection = new SqlConnection("server =920-msbi-recruitment.database.windows.net; database = RevatureDW; User Id = Chase; Password = @C_DW123@;");

            SqlCommand InsertIntoFct_Training = new SqlCommand("insert into fct_Training values(@BatchStartDate,@BatchEndDate,@waitingPeriod,@currentQLCount,1)", revDWConnection);
            SqlCommand FetchDateID = new SqlCommand("select dateID from dim_Date where date = @date", revDWConnection);

            SqlDataReader drBatchStartDate = null;
            SqlDataReader drBatchEndDate = null;

            FetchDateID.Parameters.AddWithValue("@date", BatchStartDate);

            revDWConnection.Open();
            drBatchStartDate = FetchDateID.ExecuteReader();
            if (drBatchStartDate.Read())
                InsertIntoFct_Training.Parameters.AddWithValue("@BatchStartDate", (int)drBatchStartDate[0]);
            drBatchStartDate.Close();
            revDWConnection.Close();

            FetchDateID.Parameters.Clear();
            FetchDateID.Parameters.AddWithValue("@date", BatchStartDate.AddDays(70));

            revDWConnection.Open();
            drBatchEndDate = FetchDateID.ExecuteReader();
            if (drBatchEndDate.Read())
                InsertIntoFct_Training.Parameters.AddWithValue("@BatchEndDate", (int)drBatchEndDate[0]);
            drBatchEndDate.Close();
            revDWConnection.Close();


            InsertIntoFct_Training.Parameters.AddWithValue("@waitingPeriod", WeeksWaited);
            InsertIntoFct_Training.Parameters.AddWithValue("@currentQLCount", CurrentQlCount);

            revDWConnection.Open();
            InsertIntoFct_Training.ExecuteNonQuery();
            revDWConnection.Close();

            if (Row.offerAction == "Accept")
