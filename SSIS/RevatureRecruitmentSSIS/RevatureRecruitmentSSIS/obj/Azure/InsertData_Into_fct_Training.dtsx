     *
     * Example of firing a warning event:
     *  ComponentMetaData.FireWarning(10, "Process Values", "No rows were received", "", 0);
     */
    #endregion




    /// <summary>
    /// This method is called once for every row that passes through the component from Input0.
    ///
    /// Example of reading a value from a column in the the row:
    ///  string zipCode = Row.ZipCode
    ///
    /// Example of writing a value to a column in the row:
    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        ///count of how many trainers are needed during the running time of that batch
        ///will always be atleast 1 because it includes the batch itself
        int cumulativeCount = 0;
        ///connect to the database
        SqlConnection revDWConnection = this.Connections.RevatureDWConnection.AcquireConnection(null) as SqlConnection;
        ////creation of all the SQL commands
        ///One selects all the StartDateIDs from fct_training with the same waiting period as the row were looking at
        ///the next gets all the endDateIDs
        ///The final one gets how many trainings occured during this waitingPeriod
        SqlCommand fetchStartDates = new SqlCommand("select startDateID from fct_Training where waitingPeriod = @wait", revDWConnection);
            SqlCommand fetchEndDates = new SqlCommand("select endDateID from fct_Training where waitingPeriod = @wait", revDWConnection);
            SqlCommand fetchNumberOfRows = new SqlCommand("select count(trainingID) from fct_Training where WaitingPeriod = @wait", revDWConnection);
        ///set the @wait parameter to the rows waitingPeriod were checking in all the commands
            fetchStartDates.Parameters.AddWithValue("@wait", Row.waitingPeriod);
            fetchEndDates.Parameters.AddWithValue("@wait", Row.waitingPeriod);
            fetchNumberOfRows.Parameters.AddWithValue("@wait", Row.waitingPeriod);

        ///instantiate the datareaders
            SqlDataReader drStartDates = null;
            SqlDataReader drEndDates = null;
            SqlDataReader drRowCount = null;
        ///lists that will contain the information from StartDates and EndDates for the entire dataset returned
            List<int> listStartDates = new List<int>();
            List<int> listEndDates = new List<int>();

        ///open the connection and begin reading, while reading add each of the values into the list then close the connection
        ///opening the connection the first time is done automatically by AcquireConnection
                drStartDates = fetchStartDates.ExecuteReader();
                while (drStartDates.Read())
                {
                listStartDates.Add((int)drStartDates.GetValue(0));
                }
            revDWConnection.Close();

        ///open the connection and begin reading, while reading add each of the values into the list then close the connection
        revDWConnection.Open();
                drEndDates = fetchEndDates.ExecuteReader();
                while (drEndDates.Read())
                {
                    listEndDates.Add((int)drEndDates.GetValue(0));
                }
            revDWConnection.Close();

        ///open the connection and get the number of rows, before you use drRowCount you must have an if statement otherwise it fails
            revDWConnection.Open();
                drRowCount = fetchNumberOfRows.ExecuteReader();
        if (drRowCount.Read())
        {
            ///for every row with the same waitingPeriod we check if
            ///start date is between the start date and end date + 2 weeks OR
            ///if the end date +2 weeks is between the start date and the end date between two weeks.
            ///We must check both otherwise entry 1 will always say it has 1 concurrent batch even though more get run
            ///while it is being run.
            ///I split this logic into two ifs to simplify it slightly and make it easier to look at,
            ///it could be a single if with an OR statement inside it.
            for (int x = 0; x < (int)drRowCount[0]; x++)
            {
                if (Row.startDateID >= listStartDates[x] && Row.startDateID <= (listEndDates[x]+14))
                {
                    cumulativeCount++;
                }
                else if((Row.endDateID + 14) <= (listEndDates[x] + 14) && (Row.endDateID + 14) >= listStartDates[x])
                {
                    cumulativeCount++;
                }
            }
        }
        ///close the connection and then for that row update the cumulativeCount
        revDWConnection.Close();
        Row.numConcurrentBatches = cumulativeCount;
    }

}
