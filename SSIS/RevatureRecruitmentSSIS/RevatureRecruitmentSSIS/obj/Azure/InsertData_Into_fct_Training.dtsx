#region Help:  Introduction to the Script Component
/* The Script Component allows you to perform virtually any operation that can be accomplished in
 * a .Net application within the context of an Integration Services data flow.
 *
 * Expand the other regions which have "Help" prefixes for examples of specific ways to use
 * Integration Services features within this script component. */
#endregion

#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline;
using Microsoft.SqlServer.Dts.Runtime;
using System.Data.SqlClient;
using System.Windows.Forms;
using Microsoft.SqlServer;

#endregion

/// <summary>
/// This is the class to which to add your code.  Do not change the name, attributes, or parent
/// of this class.
/// </summary>
[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    #region Help:  Using Integration Services variables and parameters
    /* To use a variable in this script, first ensure that the variable has been added to
     * either the list contained in the ReadOnlyVariables property or the list contained in
     * the ReadWriteVariables property of this script component, according to whether or not your
     * code needs to write into the variable.  To do so, save this script, close this instance of
     * Visual Studio, and update the ReadOnlyVariables and ReadWriteVariables properties in the
     * Script Transformation Editor window.
     * To use a parameter in this script, follow the same steps. Parameters are always read-only.
     *
     * Example of reading from a variable or parameter:
     *  DateTime startTime = Variables.MyStartTime;
     *
     * Example of writing to a variable:
     *  Variables.myStringVariable = "new value";
     */
    #endregion

    #region Help:  Using Integration Services Connnection Managers
    /* Some types of connection managers can be used in this script component.  See the help topic
     * "Working with Connection Managers Programatically" for details.
     *
     * To use a connection manager in this script, first ensure that the connection manager has
     * been added to either the list of connection managers on the Connection Managers page of the
     * script component editor.  To add the connection manager, save this script, close this instance of
     * Visual Studio, and add the Connection Manager to the list.
     *
     * If the component needs to hold a connection open while processing rows, override the
     * AcquireConnections and ReleaseConnections methods.
     * 
     * Example of using an ADO.Net connection manager to acquire a SqlConnection:
     *  object rawConnection = Connections.SalesDB.AcquireConnection(transaction);
     *  SqlConnection salesDBConn = (SqlConnection)rawConnection;
     *
     * Example of using a File connection manager to acquire a file path:
     *  object rawConnection = Connections.Prices_zip.AcquireConnection(transaction);
     *  string filePath = (string)rawConnection;
     *
     * Example of releasing a connection manager:
     *  Connections.SalesDB.ReleaseConnection(rawConnection);
     */
    #endregion

    #region Help:  Firing Integration Services Events
    /* This script component can fire events.
     *
     * Example of firing an error event:
     *  ComponentMetaData.FireError(10, "Process Values", "Bad value", "", 0, out cancel);
     *
     * Example of firing an information event:
     *  ComponentMetaData.FireInformation(10, "Process Values", "Processing has started", "", 0, fireAgain);
     *
     * Example of firing a warning event:
     *  ComponentMetaData.FireWarning(10, "Process Values", "No rows were received", "", 0);
     */
    #endregion
    ///variables used in the calculations of everything.
    ///counts are to determine how large a batch is and the Acceptance rate while a batch was being filled
    private int currentQlCount = 0;
    private int delayCount = 0;
    private int declineCount = 0;
    ///added 1 year to today to prevent our data from skipping in the first if statement.
    ///this should never have to be changed as we shouldnt have data beyond today in the first place.
    private DateTime batchStartDate = DateTime.Today.AddDays(365);
    ///this variable affects how many weeks a person waits, theres 3 scripts exactly the same with this as a 1,2 and 3,
    private static int weeksWaited = 1;
    private int daysWaited = 7 * weeksWaited;

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        //if we have 20 QLs in a batch then the batch has reached its max, time to enter the batch into the DB
        ///if the person were checking has a offerActDate after the start of the last batch then he's the beginning of a new batch
        ///enter that batch into the DB
        if (currentQlCount < 20 && Row.offerActDate.CompareTo(batchStartDate) < 0)
        {
            ///if the QL accepted his offer increase the number of people in the batch
            if (Row.offerAction == "Accept")
            {
                currentQlCount++;

                ///once we meet the minimum threshold for batch size set the batches start date to
                ///(7*weeksWaited)+(1 TO 6) depending on the day of the week they accepted because all batches start on a monday
                if (currentQlCount == 12)
                {
                    switch (Row.offerActDate.DayOfWeek)
                    {
                        case DayOfWeek.Monday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited);
                            break;
                        case DayOfWeek.Tuesday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 6);
                            break;
                        case DayOfWeek.Wednesday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 5);
                            break;
                        case DayOfWeek.Thursday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 4);
                            break;
                        case DayOfWeek.Friday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 3);
                            break;
                        case DayOfWeek.Saturday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 2);
                            break;
                        case DayOfWeek.Sunday:
                            batchStartDate = Row.offerActDate.AddDays(daysWaited + 1);
                            break;
                    }
                }
            }
            ///if they didnt accept just increase the counts of the other delay or decline
            else if (Row.offerAction == "Delay")
                delayCount++;
            else
                declineCount++;
        }
        else
        {
            ///connect to the database
            SqlConnection revDWConnection = this.Connections.RevatureDWConnection.AcquireConnection(null) as SqlConnection;

            ///create two sql commands, the first to insert the data into fct_Training
            ///the second to get the dateID for the batches start date and the batches end date
            SqlCommand InsertIntoFct_Training = new SqlCommand("insert into fct_Training values(@BatchStartDate,@BatchEndDate,@waitingPeriod,@currentQLCount,1,@acceptRate)", revDWConnection);
            SqlCommand FetchDateID = new SqlCommand("select dateID from dim_Date where date = @date", revDWConnection);

            ///datareaders that will hold the ID temporarily
            SqlDataReader drBatchStartDate = null;
            SqlDataReader drBatchEndDate = null;

            ///set parameter @date of FetchDateID to the batches start date
            FetchDateID.Parameters.AddWithValue("@date", batchStartDate);

            ///open the connection(this is done automatically the first time by AcquireConnection)
            ///read the data, set @batchStateDate to the DateID returned
            ///close the reader and then the connection

            drBatchStartDate = FetchDateID.ExecuteReader();
            if (drBatchStartDate.Read())
                InsertIntoFct_Training.Parameters.AddWithValue("@BatchStartDate", (int)drBatchStartDate[0]);
            drBatchStartDate.Close();
            revDWConnection.Close();

            ///clear @date of FetchDateID and get the end date ID, Trainings last 10 weeks so this is 70 days currently.
            FetchDateID.Parameters.Clear();
            FetchDateID.Parameters.AddWithValue("@date", batchStartDate.AddDays(70));

            ///open the connection, read the data, set @BatchEndDate to the dateID returned, 
            ///close the reader then the connection
            revDWConnection.Open();
            drBatchEndDate = FetchDateID.ExecuteReader();
            if (drBatchEndDate.Read())
                InsertIntoFct_Training.Parameters.AddWithValue("@BatchEndDate", (int)drBatchEndDate[0]);
            drBatchEndDate.Close();
            revDWConnection.Close();

            ///calculate and insert the remaining data for InsertIntoFct_Training
            Decimal timeAcceptanceRate = ((Decimal)currentQlCount / (Decimal)(currentQlCount + delayCount + declineCount));
            timeAcceptanceRate = Math.Round(timeAcceptanceRate, 2);
            InsertIntoFct_Training.Parameters.AddWithValue("@waitingPeriod", weeksWaited);
            InsertIntoFct_Training.Parameters.AddWithValue("@currentQLCount", currentQlCount);
            InsertIntoFct_Training.Parameters.AddWithValue("@acceptRate", timeAcceptanceRate);

            ///open the connection and insert the data into fct_Training.
            revDWConnection.Open();
            InsertIntoFct_Training.ExecuteNonQuery();
            revDWConnection.Close();

            ///reset our information for the next time period and batch,
            ///we still have to check the row we are on to determine where their data goes however,
            ///otherwise we would throw out rows
            if (Row.offerAction == "Accept")
            {
                currentQlCount = 1;
                delayCount = 0;
                declineCount = 0;
            }
            else if (Row.offerAction == "Delay")
            {
                currentQlCount = 0;
                delayCount = 1;
                declineCount = 0;
            }
            else
            {
                currentQlCount = 0;
                delayCount = 0;
                declineCount = 1;
            }
            batchStartDate = DateTime.Today.AddDays(365);
        }
    }

}
